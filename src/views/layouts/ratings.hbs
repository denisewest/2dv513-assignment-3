<div class='ratings'>
  <h1>Ratings</h1>
  <ul>
    {{#if data.length}}
    {{#each data}}
    <li>
      <div class='first_result'>
        Score {{this.score}}<br>
      </div>
      {{this.title}}<br>
      {{this.author}}<br>
      ISBN {{this.isbn}}<br>
      "{{this.review}}"<br>
      - {{this.name}}
    </li>
    {{/each}}
    {{/if}}
  </ul>
</div>