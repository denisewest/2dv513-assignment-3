<div class='ratings'>
  <h1>Ratings</h1>
  <ul>
    {{#if data.length}}
    {{#each data}}
    <li>
      {{this.title}}<br>
      {{this.author}}<br>
      {{this.isbn}}<br>
      {{this.score}}<br>
      "{{this.review}}"<br>
      - {{this.name}}
    </li>
    {{/each}}
    {{/if}}
  </ul>
</div>